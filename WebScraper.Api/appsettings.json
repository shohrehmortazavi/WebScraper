{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "MongoDbSettings": {
    "ConnectionString": "mongodb://localhost:27017",
    "DatabaseName": "CurrencyDb"
  },
  "BackgroundServicesSetting": {
    "CurrencyRateSetting": {
      "IsEnabled": true,
      "RepeatedTime": 30
    },
    "MoneyRateSetting": {
      "IsEnabled": false,
      "RepeatedTime": 30
    }
  }
}
